{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "睡眠時間": {
      "type": "object",
      "properties": {
        "時間": {
          "type": "integer",
          "minimum": 1
        },
        "分": {
          "type": "integer"
        }
      },
      "required": [
        "時間",
        "分"
      ]
    },
    "起床": {
      "type": "object",
      "properties": {
        "時": {
          "type": "integer"
        },
        "分": {
          "type": "integer"
        }
      },
      "required": [
        "時",
        "分"
      ]
    },
    "散歩": {
      "type": "object",
      "properties": {
        "実施": {
          "type": "boolean"
        },
        "ゴミ拾い": {
          "type": "boolean"
        },
        "犬遭遇": {
          "type": "object",
          "properties": {
            "数": {
              "type": "integer"
            },
            "備考": {
              "type": "string"
            }
          },
          "required": [
            "数",
            "備考"
          ]
        }
      },
      "required": [
        "実施",
        "ゴミ拾い",
        "犬遭遇"
      ]
    },
    "朝食の栄養カバレッジ": {
      "type": "string",
      "enum": [
        "0",
        "1",
        "2",
        "3"
      ]
    },
    "体操": {
      "type": "boolean"
    },
    "労働": {
      "type": "object",
      "properties": {
        "状態": {
          "type": "string",
          "enum": [
            "した",
            "休日"
          ]
        },
        "passion": {
          "type": "integer"
        },
        "discipline": {
          "type": "integer"
        },
        "備考": {
          "type": "string"
        }
      },
      "required": [
        "状態",
        "passion",
        "discipline",
        "備考"
      ]
    },
    "ジム": {
      "type": "string",
      "enum": [
        "有酸素+筋トレ",
        "有酸素",
        "休養日",
        "サボった"
      ]
    },
    "勉強会": {
      "type": "boolean"
    },
    "個人開発": {
      "type": "boolean"
    },
    "あすけん": {
      "type": "number"
    }
  },
  "required": [
    "睡眠時間",
    "起床",
    "散歩",
    "朝食の栄養カバレッジ",
    "体操",
    "労働",
    "ジム",
    "勉強会",
    "個人開発",
    "あすけん"
  ]
}