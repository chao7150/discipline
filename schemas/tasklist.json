{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
      "起床": {
        "type": "object",
        "properties": {
          "時": {
            "type": "integer",
            "minimum": 0,
            "maximum": 23,
            "title": "時"
          },
          "分": {
            "type": "integer",
            "minimum": 0,
            "maximum": 59,
            "title": "分"
          }
        },
        "required": [
          "時",
          "分"
        ]
      },
      "散歩": {
        "type": "object",
        "properties": {
          "実施": {
            "type": "boolean"
          },
          "ゴミ拾い": {
            "type": "boolean"
          },
          "犬遭遇": {
            "type": "object",
            "properties": {
              "数": {
                "type": "integer"
              },
              "備考": {
                "type": "string"
              }
            },
            "required": [
              "数",
              "備考"
            ]
          }
        },
        "required": [
          "実施",
          "ゴミ拾い",
          "犬遭遇"
        ]
      },
      "朝食": {
        "type": "object",
        "properties": {
          "三色食品群のうち": {
            "type": "integer"
          }
        },
        "required": [
          "三色食品群のうち"
        ]
      },
      "体操": {
        "type": "boolean"
      },
      "労働": {
        "type": "object",
        "properties": {
          "passion": {
            "type": "integer"
          },
          "discipline": {
            "type": "integer"
          },
          "備考": {
            "type": "string"
          }
        },
        "required": [
          "passion",
          "discipline",
          "備考"
        ]
      },
      "ジム": {
        "type": "string",
        "enum": ["有酸素+筋トレ", "有酸素", "休養日", "サボった"]
      },
      "勉強会": {
        "type": "boolean"
      },
      "個人開発": {
        "type": "boolean"
      },
      "その他": {
        "type": "array",
        "items": [
          {
            "type": "object",
            "properties": {
              "題目": {
                "type": "string"
              },
              "得点": {
                "type": "integer"
              }
            },
            "required": [
              "題目",
              "得点"
            ]
          }
        ]
      }
    },
    "required": [
      "起床",
      "散歩",
      "朝食",
      "体操",
      "労働",
      "ジム",
      "勉強会",
      "個人開発",
      "その他"
    ]
  }
